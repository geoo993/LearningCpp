#include <iostream>
#include "Cat.h"
using namespace std;


// namespaces are a way of avoiding conflict between classes or variables or even global variables that have the same name
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
