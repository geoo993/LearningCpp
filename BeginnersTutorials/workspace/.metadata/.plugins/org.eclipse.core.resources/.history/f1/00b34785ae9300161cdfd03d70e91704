#include <iostream>
#include <string>
#include <vector>

using namespace std;


vector<string> vc = {"One","Two","Three"};

int main() {

	//to work with postfix and prefix value, the value has to be an integer type

	//this is the postfix increment operator
	int value = 10;
	//value++;

	//this is prefix increment operator
	++value;
	cout << value << endl;

	for ( auto x : vc)
	{
		cout << x << endl;
	}

	return 0;
}
