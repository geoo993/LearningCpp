#include <iostream>
#include <string>
#include <vector>

using namespace std;


vector<string> vc = {"One","Two","Three"};

int main() {

	//to work with postfix and prefix value, the value has to be an integer type
	//postfix
	int i = 10;
	i++;
	cout << i << endl;

	for ( auto x : vc)
	{
		cout << x << endl;
	}

	return 0;
}
