#include <iostream>
#include <string>
#include <vector>

using namespace std;


vector<string> vc = {"One","Two","Three"};

int main() {



	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!

	for(auto x: vc)
	{

		cout << x << endl;
	}

	return 0;
}
