#include "Swarm.h"

namespace programmingparticles {

Swarm::Swarm() {
	// TODO Auto-generated constructor stub
	//cout << "SWARM CONSTRUCTOR " << endl;
	m_pParticles = new Particle[NUMBER_OF_PARTICLES];
}

Swarm::~Swarm() {
	// TODO Auto-generated destructor stub
	//cout << "SWARM DESTRUCTOR " << endl;

	delete [] m_pParticles;
}

}
