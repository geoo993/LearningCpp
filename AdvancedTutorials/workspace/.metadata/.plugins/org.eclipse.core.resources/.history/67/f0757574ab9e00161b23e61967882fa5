#include <iostream>
using namespace std;


class Test {
private:
	int id;
	string name;

public:

	Test() :
		id(0), name("") {
	}

	Test(int id, string name) :
		id(id), name(name) {
	}

	~Test() {
		//cout << "Destroyed Object" << endl;
	}

	void print() const {
		cout << name << ": " << id << endl;
	}

	bool operator<(const Test &other) const {
		return name < other.name;
	}
};


int main() {


	return 0;
}
