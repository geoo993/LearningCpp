#include <iostream>
#include "Complex.h"
using namespace std;
using namespace complexnumbering;

int main() {

	Complex c1(3.2, 5.9);
	Complex c2(7.8, 9.6);


	cout << endl;
	Complex c1(3.2, 5.9);

	return 0;
}
