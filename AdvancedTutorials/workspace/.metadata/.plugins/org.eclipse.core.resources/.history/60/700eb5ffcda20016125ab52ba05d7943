#include <iostream>
#include <vector>
using namespace std;


int main() {


	//char texts[] = {"one", "two", "three"};
	auto texts = {"one", "two", "three"};// in c++ 11 you do not need a type declaration anymore


	for(auto text: texts) {
		cout << text << endl;
	}

	vector <int> numbers;

	return 0;
}
