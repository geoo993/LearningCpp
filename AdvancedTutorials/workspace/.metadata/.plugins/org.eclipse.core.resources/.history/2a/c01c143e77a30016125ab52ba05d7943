#include <iostream>
using namespace std;

class Parent{
private:
	int id = 0;
	string name = "Aio";

public:
	Parent(){
		id = 1;
		cout << "No Parameter Parent Constructor" << endl;
	}

	Parent(string text){
		id = 2;
		this -> name = text;
	}
	~Parent(){
		cout << "Destructor" << endl;
	}
};


class Child: public Parent{

};


int main() {



	return 0;
}
