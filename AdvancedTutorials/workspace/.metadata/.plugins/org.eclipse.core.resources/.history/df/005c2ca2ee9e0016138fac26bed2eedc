#include <iostream>
using namespace std;


template<class C, class K>
class Test {

private:

	C obj1;
	K obj2;

public:
	Test(C obj1){
		this ->obj1 = obj1;
	}
	Test(C obj1, K obj2){
		this ->obj1 = obj1;
		this ->obj2 = obj2;
	}
	~Test(){};

	void print(){
		cout << obj1 << ", " << obj2 << endl;
	}

};


int main() {



	Test<string, int> test1("Mauri", 23);
	test1.print();

	cout << endl;
	return 0;
}
