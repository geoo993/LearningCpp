#include <iostream>
#include <stack>
#include <queue>

using namespace std;

class Test {
private:
	int id;
	string name;

public:

	Test() : id(0), name("") {}

	Test(int id, string name) : id(id), name(name) {}

	~Test()  {
		cout << "Destroyed Object" << endl;
	}

	void print() const {
		//cout << id << ": " << name << endl;
		cout << name << ": " << id << endl;
	}

	bool operator<(const Test &other) const {
		//return id < other.id;
		return name < other.name;
	}
};


int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
