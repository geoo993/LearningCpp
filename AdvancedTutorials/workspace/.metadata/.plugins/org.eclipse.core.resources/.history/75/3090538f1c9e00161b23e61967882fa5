#include <iostream>
using namespace std;


class Person {

private:
	string name;
	int age;

public:

	Person(string name, int age): name(name), age(age){}
	void print() const { cout<< name << age << endl; }

};

int main() {

	map <int, Person> people;
	people[1] = Person("Ziggy", 24);
	people[1] = Person("Geo",23);
	people[1] = Person("Raj", 56);
	people[1] = Person("Zhan", 25);



	cout << "!!!Hello World!!!" << endl;

	return 0;
}
