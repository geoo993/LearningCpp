#include <iostream>
using namespace std;

class Sport{
public:
	void print(){
		cout << "Sport" << endl;
	}
};

class Tennis: Sport {
public:
	void print(){
		cout << "Tennis" << endl;
	}
};

class Rugby: Sport {

};



int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
