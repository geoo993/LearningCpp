#include <iostream>
using namespace std;


//unique pointer are kind of smart pointer, behaves like normal pointer except they handle the deallocation of memory for you

class Test {
public:
	Test(){
		cout << "Constructor to create" << endl;
	}
	void print(){

	}


	~Test(){
		cout << "Destructor to destroy" << endl;
	}
};

int main() {

	return 0;
}
