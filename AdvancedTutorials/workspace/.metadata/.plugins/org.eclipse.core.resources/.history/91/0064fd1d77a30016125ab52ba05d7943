#include <iostream>
using namespace std;

class Parent{
private:
	int id = 0;
	string name = "Aio";

public:
	Parent(){
		id = 5;
		cout << "" << endl;
	}
};


class Child: public Parent{

};


int main() {



	return 0;
}
