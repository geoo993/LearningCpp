#ifndef COMPLEX_H_
#define COMPLEX_H_

namespace complexnumbering {

class Complex {
private:

	double real;
	double imaginary;

public:
	Complex();
	Complex(double real, double imaginary);
	Complex(const Test &other);
	const Complex &operator=(const Complex &other) {

		cout << "Assignment running" << endl;
		id = other.id;
		name = other.name;

		return *this;
	}

	virtual ~Complex();
};

}

#endif /* COMPLEX_H_ */
