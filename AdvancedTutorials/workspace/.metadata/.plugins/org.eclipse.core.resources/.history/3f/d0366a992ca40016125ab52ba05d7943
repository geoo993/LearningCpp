#include <iostream>
using namespace std;

//return values
Test getTest(){
	return Test();
}

void check(const Test &value){

	cout << "L value function." << endl;
}

void check(Test &&value){

	cout << "R value function." << endl;
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
