#ifndef PERSON_H_
#define PERSON_H_
#include <string>
#include <iostream>

class Person {
private:
	std::string name;
	std::string age;
	std::string gender;


public:
	Person();
	~Person();
	void setPerson(string name, string age, string gender);
	void print();
	string getName();
	void setName(string newName);

};

#endif /* PERSON_H_ */
