#ifndef ANIMAL_H_
#define ANIMAL_H_
#include <iostream>
using namespace std;

class Animal {
private:
	string name;
	string group;

public:
	Animal();
	virtual ~Animal();
	Animal(string name, string group): name (name), group(group){}
	void setAnimal(string name, string group): name (name), group(group){}
	void print();
};

#endif /* ANIMAL_H_ */
