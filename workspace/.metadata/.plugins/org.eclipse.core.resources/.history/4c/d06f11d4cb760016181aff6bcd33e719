#ifndef ANIMALS_H_
#define ANIMALS_H_

#include <iostream>
using namespace std;


namespace toCat {

class Dog {
public:
	Dog();
	virtual ~Dog();
	Dog(string name): name(name){};
	void print(){ cout << name << " is a dog." << endl;};
};

}

#endif /* ANIMALS_H_ */
