#include <iostream>
#include "Cat.h"
using namespace std;

int main() {

	Cat lina;
	string linaMood = lina.makeCatHappy();
	cout << linaMood << endl;


	Cat max;
	string maxMood = max.makeCatSad();
	cout << maxMood << endl;

	return 0;
}
