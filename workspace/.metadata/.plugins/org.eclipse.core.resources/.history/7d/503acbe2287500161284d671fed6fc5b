#ifndef PERSON_H_
#define PERSON_H_
#include <string>

class Person {
private:
	std::string name;
	std::string age;
	std::string gender;


public:
	Person();
	void Person(std::string name, std::string age, std::string gender);
	void setPerson(std::string name, std::string age, std::string gender);
	void print();

};

#endif /* PERSON_H_ */
