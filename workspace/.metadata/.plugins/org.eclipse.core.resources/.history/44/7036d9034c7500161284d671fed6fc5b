#include <iostream>
#include "Fruit.h"
using namespace std;

int main() {

	Fruit fruit1("Apple", "Green");
	Fruit fruit2("Banana", "Yellow");

	cout << fruit1.toString() << "Memory Location of this is " << &fruit1 endl;// to get the actual memory location of an object,
	cout << fruit2.toString() << endl;
	return 0;
}
