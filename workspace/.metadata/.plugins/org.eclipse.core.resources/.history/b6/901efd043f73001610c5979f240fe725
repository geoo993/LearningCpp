#include <iostream>
using namespace std;

int main() {


	string text = "Hello";

	cout << text << endl; // cout is also an object and its a special object for outputing text,
	cout << "Hello World" << endl;//the chevron (<<) is called the insertion operator, they are used to form an operation which is this case is for outputting text

	return 0;
}
