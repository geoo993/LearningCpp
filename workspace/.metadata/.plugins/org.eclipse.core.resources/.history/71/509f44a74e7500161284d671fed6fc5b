#ifndef TRAINERS_H_
#define TRAINERS_H_

#include <iostream>
#include <sstream>

using namespace std;


class Trainers {
private:
	string name;
	string color;
	string brand;
	double price;
public:
	Trainers();
	virtual ~Trainers();
	Trainers(string name, string color, string brand, double price);
};

#endif /* TRAINERS_H_ */
