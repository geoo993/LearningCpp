#include <iostream>
#include <string>
#include "Cat.h"
using namespace std;

std::string Cat::noise() {

	if (happy) {
		return "Meeaaoowww";
	} else {
		return "Shhhsshshh";
	}
}

std::string Cat::makeCatHappy() {
	happy = true;
}

std::string Cat::makeCatSad() {
	happy = false;
}
