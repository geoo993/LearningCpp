#include <iostream>
#include <sstream>

#include "Trainers.h"
using namespace std;

int main() {

	Trainers trainers1("Air Forces", "Gray", "Nike", 59.99);
	Trainers trainers2("Mens Converses", "White", "Converse", 49.99);

