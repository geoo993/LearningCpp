#include <iostream>
using namespace std;

class Vehicle {

private:
	string name;
	string machineType;


public:
	//this is the class constructor
	Vehicle(string name="", string machineType=""): name(name), machineType(machineType) {
		cout << "Machine constructor called" << endl;
	}
	void print(){cout << "" << endl;}
};


int main() {

	Vehicle machine;

	return 0;
}
